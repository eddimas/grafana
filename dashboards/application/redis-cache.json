{
  "id": null,
  "title": "Redis Cache Monitoring (Immich)",
  "tags": ["cache", "redis", "immich"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "annotations": { "list": [] },
  "templating": { "list": [] },
  "panels": [
    {
      "id": 1,
      "title": "Redis Pods Up (immich-dev)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up{namespace=\"immich-dev\", pod=~\".*redis.*\"}) or vector(0)",
          "legendFormat": "Pods Up"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "DOWN" },
                "1": { "text": "UP" }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Redis CPU (cores)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"immich-dev\", pod=~\".*redis.*\", image!=\"\"}[5m])) or vector(0)",
          "legendFormat": "CPU"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "cores",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "title": "Redis Memory (bytes)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(container_memory_working_set_bytes{namespace=\"immich-dev\", pod=~\".*redis.*\", image!=\"\"}) or vector(0)",
          "legendFormat": "Mem"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "title": "Redis Network I/O (bytes/s)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(rate(container_network_receive_bytes_total{namespace=\"immich-dev\", pod=~\".*redis.*\"}[5m])) + sum(rate(container_network_transmit_bytes_total{namespace=\"immich-dev\", pod=~\".*redis.*\"}[5m]))) or vector(0)",
          "legendFormat": "Net"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "Bps", "min": 0 }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
    },
    {
      "id": 5,
      "title": "CPU Over Time (Redis pods)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{namespace=\"immich-dev\", pod=~\".*redis.*\", image!=\"\"}[5m]))",
          "legendFormat": "{{pod}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "cores", "min": 0 } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 6,
      "title": "Memory Over Time (Redis pods)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (pod) (container_memory_working_set_bytes{namespace=\"immich-dev\", pod=~\".*redis.*\", image!=\"\"})",
          "legendFormat": "{{pod}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes", "min": 0 } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
    },
    {
      "id": 7,
      "title": "Redis Cache Hit/Miss (requires redis exporter)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(irate(redis_keyspace_hits_total{namespace=\"immich-dev\"}[5m]))",
          "legendFormat": "hits/sec"
        },
        {
          "refId": "B",
          "expr": "sum(irate(redis_keyspace_misses_total{namespace=\"immich-dev\"}[5m]))",
          "legendFormat": "misses/sec"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "min": 0 } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 }
    },
    {
      "id": 8,
      "title": "Network Over Time (Redis pods)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (pod) (rate(container_network_receive_bytes_total{namespace=\"immich-dev\", pod=~\".*redis.*\"}[5m]))",
          "legendFormat": "{{pod}} RX"
        },
        {
          "refId": "B",
          "expr": "sum by (pod) (rate(container_network_transmit_bytes_total{namespace=\"immich-dev\", pod=~\".*redis.*\"}[5m]))",
          "legendFormat": "{{pod}} TX"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0 } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 }
    },
    {
      "id": 9,
      "title": "Redis Keys (requires redis exporter)",
      "type": "table",
      "targets": [
        {
          "refId": "A",
          "expr": "redis_db_keys{namespace=\"immich-dev\"}",
          "legendFormat": "{{db}}"
        }
      ],
      "options": { "showHeader": true },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 20 }
    },
    {
      "id": 10,
      "title": "Evicted Keys (1h) (requires redis exporter)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(redis_evicted_keys_total{namespace=\"immich-dev\"}[1h])) or vector(0)",
          "legendFormat": "evicted"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 20 }
    }
  ]
}
